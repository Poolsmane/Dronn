function validateBuyBack()
    {
        var buyback_param_val = $('.buyback_param_val').val();
        var buyback_description = $('.buyback_description').val();
        var buyback_serial_no = $('.buyback_serial_no').val();
        var buyback_model = $('.buyback_model').val();
        var buyback_brand = $('.buyback_brand').val();
        var buyback_yom = $('.buyback_yom').val();
        var buyback_purchased_on = $('.buyback_purchased_on').val();
        var buyback_age = $('.buyback_age').val();
        var buyback_size = $('.buyback_size').val();
        var buyback_rating = $('.buyback_rating').val();
        var buyback_quantity = $('.buyback_quantity').val();
        var buyback_unit = $('.buyback_unit').val();
        var buyback_remark = $('.buyback_remark').val();
        var buyback_uploaded_image = $('.buyback_uploaded_image').prop('files')[0];
        var buyback_uploaded_name = $('#buyback_uploaded_name').val();
        var year = new Date().getFullYear();
        $(".error").remove();
        var err = 0;
        //alert(buyback_uploaded_image);
        if(buyback_purchased_on.length>1){
            var purchaseyear = buyback_purchased_on.split("-");
            var purchase_year = purchaseyear['2'];
        }
        $('.buyback_param_val').after('');
        if((buyback_param_val.length < 3) || (buyback_param_val.length > 25))
        {
            $('.buyback_param_val').after('<span class="error">Item details must be between 3 and 25 characters.</span>');
            err++;
        }
        $('.buyback_description').after('');
        if((buyback_description.length < 1) || (buyback_description.length > 150))
        {
            $('.buyback_description').after('<span class="error">Item description must be between 1 and 150 characters.</span>');
             err++;
        }
        $('.buyback_serial_no').after('');
        if((buyback_serial_no.length < 2) || (buyback_serial_no.length > 50))
        {
            $('.buyback_serial_no').after('<span class="error">Serial number must be between 2 and 50 characters.</span>');
             err++;
        }
        if((buyback_model.length < 2) || (buyback_model.length > 50))
        {
            $('.buyback_model').after('<span class="error">Model number must be between 2 and 50 characters.</span>');
             err++;
        }
        $('.buyback_brand').after('');
        if((buyback_brand.length < 2) || (buyback_brand.length > 50))
        {
            $('.buyback_brand').after('<span class="error">Brand details must be between 2 and 50 characters.</span>');
             err++;
        }
        $('.buyback_yom').after('');
        if((buyback_yom.length < 4) || (buyback_yom.length > 4))
        {
            $('.buyback_yom').after('<span class="error">Please enter a valid year of manufacturing and must contain 4 digits.</span>');
             err++;
        }
        else if((buyback_yom.length == 4) && (buyback_yom < '1950'))
        {
            $('.buyback_yom').after('<span class="error">Please enter a valid year of manufacturing and must be greater than or equal to 1950 and contain 4 digits.</span>');
             err++;
        }
        else if((buyback_yom.length == 4) && (buyback_yom > year))
        {
            $('.buyback_yom').after('<span class="error">Please enter a valid year of manufacturing and must be smaller than or equal to current year and contain 4 digits.</span>');
             err++;
        }
        $('.buyback_purchased_on').after('');
        if(buyback_purchased_on.length<1)
        {
            $('.buyback_purchased_on').after('<span class="error">Please select valid Date Of Purchase.</span>');
             err++;
        }
        else if(purchase_year<'1950')
        {
             $('.buyback_purchased_on').after('<span class="error">Please select valid Date Of Purchase must be greater than or equal to 1950.</span>');
             err++;
        }
        $('.buyback_size').after('');
        if(buyback_size.length > 50){
            $('.buyback_size').after('<span class="error">Size must be between 1 and 50 characters.</span>');
             err++;
        }
        $('.buyback_quantity').after('');
        if(buyback_quantity < 1 || buyback_quantity > 999999999)
        {
            $('.buyback_quantity').after('<span class="error">Quantity must be between 1 and 999999999.</span>');
             err++;
        }
        if((buyback_unit.length < 2) || (buyback_unit.length > 50))
        {
            $('.buyback_unit').after('<span class="error">Unit Of Measurement must be between 2 and 50 characters.</span>');
             err++;
        }
        if((buyback_remark.length !='' ) && (buyback_remark.length > 250))
        {
            $('.buyback_remark').after('<span class="error">Remark must not exceed 250 characters.</span>');
             err++;
        }
        if(buyback_uploaded_name.length < 1)
        {
            $('.buy_file_upload').after('<span class="error">Please upload a valid image. Only PNG/JPG/JPEG is allowed up to 10MB</span>');
             err++;
        }
        return err;
    }
    $(document).ready(function() {
        $('.time_edit_class').click(function(){
            if($('.edit_hour_time').hasClass('in')){
               $('.time_edit_class').removeClass("date-time-btn-disabled");
            }else{
               $('.time_edit_class').addClass("date-time-btn-disabled");
            }
        });
});
</script>

