<script>
    function checkLoadData() {
        var loadbased_required = $('input[name="loadbased_required"]:checked').val();

        if(loadbased_required == undefined || loadbased_required == 'undefined')
        {
            $('.loadbased_error').html('This is required');
            return false;
        }

        if((loadbased_required != undefined) && (loadbased_required == 1))
        {
            $('.red').html('');
            var loadbased_reason = $(".loadbased_reason").val().trim();
            if(loadbased_reason == '') {
                $('.loadbased_reason_error').html('This is required');
                return false;
            }
            if(loadbased_reason.length < 50 || loadbased_reason.length > 5000) {
                $('.loadbased_reason_error').html('Reason must be between 50 to 5000 characters');
                return false;
            }

            var loadbased_doc_required = $('input[name="loadbased_doc_required"]:checked').val();

            if(loadbased_doc_required == undefined || loadbased_doc_required == 'undefined')
            {
                $('.loadbased_doc_error').html('This is required');
                return false;
            }

            if((loadbased_doc_required != undefined) && (loadbased_doc_required == 1))
            {
                var loadbased_file = $("#loadbased_file").val();
                if(loadbased_file == '' && $("#prev_loadbased_file").val() == '') {
                    $('.loadbased_file_error').html('This is required');
                    return false;
                }
            }

        } 
        return true;
    }

    $(document).on('change', '#loadbased_file', function (e) {
        const file = document.getElementById($(this).attr('id')); 
        $('.loadbased_file_error').html('');
        if(file.files.length > 0) {
            const fSize = file.files.item(0).size;
            const fName = file.files.item(0).name;
            const fExt = fName.split('.')[fName.split('.').length - 1].toLowerCase();
            
            if(fSize > 0 && fSize <= 5242880) {
                if(fExt != 'pdf') {
                    alert("Please upload document in PDF file only");
                    $(this).val('')
                }
            } else {
                alert("Maximum supported file size for upload is 5 MB");
                $(this).val('')
            }
        } else {
            alert("No file selected");
            $(this).val('')
        }
    });

    $(document).ready(function () {
    $(document).on("click", '#btn_update_loadbased:visible', function (e) {
        if(checkLoadData()){
            var loadfile = $("#loadbased_file").val();
            var load_bid_file = $("#prev_loadbased_file").val();
            var load_file_required =$('input[name="loadbased_doc_required"]:checked').val();
            if(load_file_required == 1) {
                if(loadfile != '') {
                    load_bid_file = uploadLoadDoc();
                }
            } else {
                load_bid_file = '';
            }
            var loadbased_required = $('input[name="loadbased_required"]:checked').val();
            var loadbased_reason = $(".loadbased_reason").val().trim();
            var bd_id = $("#bd_id").val();
            var post_array = {"load_bid_file":load_bid_file,"bd_id":bd_id,"loadbased_required":loadbased_required,"loadbased_reason":loadbased_reason,'csrf_bd_gem_nk': 'c3a510d97157e2ba22a116c147e49517'};
            $.ajax({
                url: "https://bidplus.gem.gov.in/bidding/updateLoadData/",
                async: false,
                type: "POST",
                data: post_array,
                dataType: "json",
                success: function (data) {
                    if(!data.status) {
                        $('.loadbased_error').html(data.message);
                    } else {
                        location.reload();
                    }
                },
                error: function () {
                    alert('Something went wrong, Please try again!');
                }
            });
        }
    });
    });

    function uploadLoadDoc() {
        var bidID = $('#bidID').val();
        var fd = new FormData();
        fd.append("loadbased_file", document.getElementById("loadbased_file").files[0]);
        fd.append('bid_id', bidID);
        fd.append('csrf_bd_gem_nk', 'c3a510d97157e2ba22a116c147e49517');
        var saveLoadDocUrl = "https://bidplus.gem.gov.in/bidding/buyerbid/saveLoadDoc/";
        var load_bid_file = '';
        $.ajax({
            url: saveLoadDocUrl,
            async: false,
            type: "POST",
            data: fd,
            dataType: "json",
            contentType: false,
            processData: false,
            success: function (data) {
                if (data.status)
                {
                    load_bid_file = data.data;
                }
            }
        });
        return load_bid_file;
    }
    

